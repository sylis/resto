.page-header
  h1 New restaurant

- form_for(restaurant, {action: path_to.restaurants, method: 'POST', id: "restaurant_form"}, function (form) {
  != partial('restaurants/form.jade', {locals: {form: form, restaurant: restaurant}})
  .actions
    != form.submit('Create restaurant', {class: 'btn'})
    span= ' or '
    != link_to('Cancel', path_to.restaurants, {class: 'btn'})
- });
